{
  "name": "Snipe59",
  "version": "1.4.0",
  "description": "FIFA23 AutoBuyer by FUTstarz\n\n\n",
  "manifest_version": 2,
  "web_accessible_resources": [
    "injected.js",
    "js/jquery-3.5.1.min.js"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.ea.com/*"
      ],
      "js": [
        "js/jquery-3.5.1.min.js",
        "js/content.js",
        "js/jqueryui/jquery-ui.js",
        "js/jqueryui/external/jquery/jquery.js",
        "js/ga.js"

      ],
      "css": [
        "css/main.css",
        "css/injected.css"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "images/new/16.png",
    "48": "images/new/48.png",
    "128": "images/new/128.png"
  },
  "permissions": [
    "webRequest","tabs", "*://*.ea.com/*",    "storage",
    "https://*.futsovereign.com/*",
    "https://*.futstarz.com/*"
  ],
  "background":{
    "scripts":["js/background.js" ]
  }
}
